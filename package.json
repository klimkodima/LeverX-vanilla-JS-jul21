{
  "name": "leverx-vanillajs-template",
  "description": "Template for frontend vanilla JS homeworks",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint-fix": "npm run lint -- --fix",
    "start": "live-server --open=\"./src/index.html\"",
    "star": "cross-env NODE_ENV=development nodemon server.js",
    "test": "cross-env NODE_ENV=test babel-tape-runner test/test-*.js",
    "pretest": "eslint .",
    "prod": "pm2-runtime start ecosystem.config.js --env production"
  },
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "co": "^4.6.0",
    "compression": "~1.7.4",
    "connect-flash": "~0.1.1",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "cross-env": "~6.0.3",
    "csurf": "1.10.0",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "express-session": "~1.17.0",
    "fs-extra": "^10.0.0",
    "helmet": "~3.21.2",
    "method-override": "~3.0.0",
    "mongoose": "^5.12.15",
    "morgan": "~1.9.1",
    "only": "0.0.2",
    "passport": "~0.4.1",
    "passport-local": "~1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pm2": "~4.2.1",
    "pug": "^3.0.2",
    "view-helpers": "~0.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.9.0",
    "eslint-plugin-prettier": "~3.1.2",
    "husky": "^4.2.1",
    "live-server": "^1.2.1",
    "prettier": "~1.19.1",
    "babel-eslint": "~10.0.3",
    "babel-tape-runner": "~3.0.0",
    "nodemon": "2.0.2",
    "npm-check-updates": "~4.0.1",
    "supertest": "*",
    "tape": "~4.12.1"
  },
  "husky": {
    "hooks": {
      
    }
  },
  "keywords": [
    "node",
    "mongoose",
    "express"
  ]
}
